package cyjroxcode;

public class WhileCounter {

	public static void main(String[] args) {
	
		int counter = 1;
		
while(counter <= 100)
{
	System.out.printf("%4d ", counter);
	++counter;
}
		System.out.println();
		
	int counter2 = 1;
while(++counter <= 100);
{
	System.out.printf(" %2d%n ", counter2);
	++counter2;
	}
	System.out.println();

}
}