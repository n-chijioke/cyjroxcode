package cyjroxcode;

public class AsteriskCode{
	
	public static void main(String[] args) {
		
		for(int d = 1; d < 7; d++) {
			
			for(int c = 1; c < 7; c++) {
				System.out.print("*");
			}
			if(d = 1.| |. d == 7) {
			System.out.print("*");
			}else {
				System.out.print("*");
				System.out.print(" ");
				System.out.print(" ");
				System.out.print(" ");
				System.out.print(" ");
				System.out.print(" ");
			}
			System.out.print("*");	
			}
		System.out.println();
		}
		
		
		
	}
}
