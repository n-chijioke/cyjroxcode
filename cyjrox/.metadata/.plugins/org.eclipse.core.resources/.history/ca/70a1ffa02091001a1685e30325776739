package cyjroxcode.copy;

public class Asterisk {

	public static void main(String[] args) {
		
		
	for(int i = 0; i <= 5; i++) {
			
		for( int number = 1; number <= 4; number++){
				System.out.printf("@");
				System.out.printf("^");
			}
					System.out.println();
			
				System.out.println();
		}
	}

}
