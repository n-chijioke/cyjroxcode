package cyjroxcode.copy;

public class Asterisk {

	public static void main(String[] args) {
		
		
	for(int i = 0; i <= 5; i++) {
			
		for( int number = 1; number <= 4; number++){
				System.out.printf("@");
		}
			for(int d = 1; d <=4; d++) {
				System.out.printf("^" + "i");
			}
			
				System.out.println();
		}
	}

}
